{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug fsm",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/local/mppxfr/fsm/fsm",
            "args": [6002],
            "stopAtEntry": false,
            "cwd": "/usr/local/mppxfr/fsm",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "Build fsm",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
